{
  "prettier.singleQuote": false,
  "files.autoSave": "off",
  "extensions.autoUpdate": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.tabWidth": 4,
  "prettier.useTabs": true,
  "prettier.printWidth": 1000000,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#fffafa",
    "activityBar.background": "#0faeb7",
    "activityBar.foreground": "#2c2a2a",
    "activityBar.inactiveForeground": "#fff",
    "activityBarBadge.background": "#c8f7ea",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#21a9d2",
    "statusBar.background": "#30e5eb",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0faeb7",
    "statusBarItem.remoteBackground": "#0faeb7",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#0faeb7",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#0faeb7",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#a37c18",
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 100,
  "cSpell.words": ["bkash", "cloudinary", "nagad", "uuidv"]
}
